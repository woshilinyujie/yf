        <!-- 选择业务员 -->
        <activity android:name=".Activity.Order.SelectSalesmanActivity"/>
        <activity android:name=".Activity.My.PhotoActivity"/>
        <!-- 库存 -》筛选 -->
        <activity android:name=".Activity.Stock.FilterListActivity"/>
