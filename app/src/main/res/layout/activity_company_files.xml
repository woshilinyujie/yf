<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/c_f6f7fb"
    android:orientation="vertical">

    <jh.zkj.com.yf.Mview.TitleLayout
        android:layout_width="match_parent"
        android:layout_height="@dimen/dp_48"
        android:background="@color/white"
        app:centerText="公司档案"
        app:centerTextColor="@color/c_333333"
        app:centerTextSize="@dimen/sp_17"
        app:leftImageBg="@mipmap/back_arrow"
        app:leftImageHeightSize="@dimen/dp_20"
        app:leftImageWidthSize="@dimen/dp_12"
        app:leftMargin="@dimen/dp_16"
        app:rightMargin="@dimen/dp_16"
        app:rightText="保存"
        app:rightTextColor="@color/c_666666"
        app:rightTextSize="@dimen/sp_14" />

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="@dimen/dp_44"
        android:background="@color/white">

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerVertical="true"
            android:layout_marginLeft="@dimen/dp_6"
            android:gravity="center"
            android:text="*"
            android:textColor="@color/c_ff0000"
            android:textSize="@dimen/sp_14" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerVertical="true"
            android:layout_marginLeft="@dimen/dp_16"
            android:text="公司名称"
            android:textColor="@color/c_333333"
            android:textSize="@dimen/sp_14" />

        <EditText
            android:id="@+id/eew"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_centerVertical="true"
            android:layout_marginLeft="@dimen/dp_110"
            android:background="@null"
            android:hint="请输入公司名称"
            android:textColor="@color/c_333333"
            android:textColorHint="@color/c_d9d9d9"
            android:textSize="@dimen/sp_14" />

    </RelativeLayout>

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="@dimen/dp_44"
        android:layout_marginTop="@dimen/dp_1"
        android:background="@color/white">


        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerVertical="true"
            android:layout_marginLeft="@dimen/dp_16"
            android:maxEms="5"
            android:text="统一社会信用代码"
            android:textColor="@color/c_333333"
            android:textSize="@dimen/sp_14" />

        <EditText

            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_centerVertical="true"
            android:layout_marginLeft="@dimen/dp_110"
            android:background="@null"
            android:hint="请输入统一社会信用代码"
            android:textColor="@color/c_333333"
            android:textColorHint="@color/c_d9d9d9"
            android:textSize="@dimen/sp_14" />

    </RelativeLayout>

    <RelativeLayout
        android:layout_marginTop="@dimen/dp_1"
        android:layout_width="match_parent"
        android:layout_height="@dimen/dp_44"
        android:background="@color/white">

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerVertical="true"
            android:layout_marginLeft="@dimen/dp_6"
            android:gravity="center"
            android:text="*"
            android:textColor="@color/c_ff0000"
            android:textSize="@dimen/sp_14" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerVertical="true"
            android:layout_marginLeft="@dimen/dp_16"
            android:text="所属行业"
            android:textColor="@color/c_333333"
            android:textSize="@dimen/sp_14" />

        <TextView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_centerVertical="true"
            android:layout_marginLeft="@dimen/dp_110"
            android:text="数码3C/手机"
            android:textColor="@color/c_a6a6a6"
            android:textSize="@dimen/sp_14" />

    </RelativeLayout>

    <RelativeLayout
        android:layout_marginTop="@dimen/dp_1"
        android:layout_width="match_parent"
        android:layout_height="@dimen/dp_44"
        android:background="@color/white">

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerVertical="true"
            android:layout_marginLeft="@dimen/dp_6"
            android:gravity="center"
            android:text="*"
            android:textColor="@color/c_ff0000"
            android:textSize="@dimen/sp_14" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerVertical="true"
            android:layout_marginLeft="@dimen/dp_16"
            android:text="所在地区"
            android:textColor="@color/c_333333"
            android:textSize="@dimen/sp_14" />

        <TextView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_centerVertical="true"
            android:layout_marginLeft="@dimen/dp_110"
            android:background="@null"
            android:text="请选择地区"
            android:textColor="@color/c_d9d9d9"
            android:textSize="@dimen/sp_14" />

        <ImageView
            android:background="@mipmap/right_lab"
            android:layout_alignParentRight="true"
            android:layout_centerVertical="true"
            android:layout_marginRight="@dimen/dp_16"
            android:layout_width="@dimen/dp_6"
            android:layout_height="@dimen/dp_12" />

    </RelativeLayout>
    <RelativeLayout
        android:layout_marginTop="@dimen/dp_1"
        android:layout_width="match_parent"
        android:layout_height="@dimen/dp_60"
        android:background="@color/white">
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerVertical="true"
            android:layout_marginLeft="@dimen/dp_6"
            android:gravity="center"
            android:text="*"
            android:textColor="@color/c_ff0000"
            android:textSize="@dimen/sp_14" />


        <TextView
            android:layout_marginTop="@dimen/dp_16"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="@dimen/dp_16"
            android:text="详细地址"
            android:textColor="@color/c_333333"
            android:textSize="@dimen/sp_14" />

        <EditText
            android:paddingRight="@dimen/dp_16"
            android:inputType="textMultiLine"
            android:layout_marginTop="@dimen/dp_16"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginLeft="@dimen/dp_110"
            android:background="@null"
            android:hint="请输入详细地址"
            android:textColor="@color/c_333333"
            android:textColorHint="@color/c_d9d9d9"
            android:textSize="@dimen/sp_14" />


    </RelativeLayout>

    <RelativeLayout
        android:layout_marginTop="@dimen/dp_1"
        android:layout_width="match_parent"
        android:layout_height="@dimen/dp_44"
        android:background="@color/white">

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerVertical="true"
            android:layout_marginLeft="@dimen/dp_16"
            android:text="法定代表人"
            android:textColor="@color/c_333333"
            android:textSize="@dimen/sp_14" />

        <EditText

            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_centerVertical="true"
            android:layout_marginLeft="@dimen/dp_110"
            android:background="@null"
            android:hint="请输入法定代表人"
            android:textColor="@color/c_333333"
            android:textColorHint="@color/c_d9d9d9"
            android:textSize="@dimen/sp_14" />

    </RelativeLayout>
    <RelativeLayout
        android:layout_marginTop="@dimen/dp_1"
        android:layout_width="match_parent"
        android:layout_height="@dimen/dp_44"
        android:background="@color/white">

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerVertical="true"
            android:layout_marginLeft="@dimen/dp_16"
            android:text="联系人"
            android:textColor="@color/c_333333"
            android:textSize="@dimen/sp_14" />

        <EditText

            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_centerVertical="true"
            android:layout_marginLeft="@dimen/dp_110"
            android:background="@null"
            android:hint="请输入联系人"
            android:textColor="@color/c_333333"
            android:textColorHint="@color/c_d9d9d9"
            android:textSize="@dimen/sp_14" />

    </RelativeLayout>
    <RelativeLayout
        android:layout_marginTop="@dimen/dp_1"
        android:layout_width="match_parent"
        android:layout_height="@dimen/dp_44"
        android:background="@color/white">

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerVertical="true"
            android:layout_marginLeft="@dimen/dp_16"
            android:text="联系电话"
            android:textColor="@color/c_333333"
            android:textSize="@dimen/sp_14" />

        <EditText

            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_centerVertical="true"
            android:layout_marginLeft="@dimen/dp_110"
            android:background="@null"
            android:hint="请输入联系电话"
            android:textColor="@color/c_333333"
            android:textColorHint="@color/c_d9d9d9"
            android:textSize="@dimen/sp_14" />

    </RelativeLayout>
    <RelativeLayout
        android:layout_marginTop="@dimen/dp_1"
        android:layout_width="match_parent"
        android:layout_height="@dimen/dp_44"
        android:background="@color/white">

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerVertical="true"
            android:layout_marginLeft="@dimen/dp_16"
            android:text="邮编"
            android:textColor="@color/c_333333"
            android:textSize="@dimen/sp_14" />

        <EditText

            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_centerVertical="true"
            android:layout_marginLeft="@dimen/dp_110"
            android:background="@null"
            android:hint="请输入邮编"
            android:textColor="@color/c_333333"
            android:textColorHint="@color/c_d9d9d9"
            android:textSize="@dimen/sp_14" />

    </RelativeLayout>

    <RelativeLayout
        android:layout_marginTop="@dimen/dp_1"
        android:layout_width="match_parent"
        android:layout_height="@dimen/dp_100"
        android:background="@color/white">

        <TextView
            android:textColor="@color/c_333333"
            android:layout_marginLeft="@dimen/dp_15"
            android:layout_marginTop="@dimen/dp_15"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="上传身份证" />


        <ImageView
            android:background="@mipmap/business"
            android:layout_marginTop="@dimen/dp_15"
            android:layout_marginLeft="@dimen/dp_112"
            android:layout_width="@dimen/dp_106"
            android:layout_height="@dimen/dp_70" />

    </RelativeLayout>
</LinearLayout>