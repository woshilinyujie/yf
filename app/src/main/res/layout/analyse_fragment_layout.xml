<?xml version="1.0" encoding="utf-8"?>
<LinearLayout android:orientation="vertical"
    android:background="@color/c_f6f7fb"
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto">

    <jh.zkj.com.yf.Mview.TitleLayout
        android:id="@+id/phone_black"
        android:layout_width="match_parent"
        android:layout_height="@dimen/dp_44"
        android:background="@color/white"
        app:centerText="经营分析"
        app:centerTextColor="@color/c_333333"
        app:centerTextSize="@dimen/sp_17"
       />


    <RelativeLayout
        android:id="@+id/analyse_shop_rl"
        android:layout_marginTop="@dimen/dp_10"
        android:background="@color/white"
        android:layout_width="match_parent"
        android:layout_height="@dimen/dp_44">

        <ImageView
            android:layout_marginLeft="@dimen/dp_17"
            android:layout_centerVertical="true"
            android:background="@mipmap/shop"
            android:layout_width="@dimen/dp_16"
            android:layout_height="@dimen/dp_15" />

        <TextView
            android:layout_marginLeft="@dimen/dp_44"
            android:layout_centerVertical="true"
            android:text="门店经营分析"
            android:textColor="@color/c_333333"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" />

        <ImageView
            android:layout_marginRight="@dimen/dp_16"
            android:layout_alignParentRight="true"
            android:layout_centerVertical="true"
            android:background="@mipmap/right_lab"
            android:layout_width="@dimen/dp_6"
            android:layout_height="@dimen/dp_12" />
    </RelativeLayout>
    <RelativeLayout
        android:id="@+id/analyse_shop_man_rl"
        android:layout_marginTop="@dimen/dp_10"
        android:background="@color/white"
        android:layout_width="match_parent"
        android:layout_height="@dimen/dp_44">

        <ImageView
            android:layout_marginLeft="@dimen/dp_17"
            android:layout_centerVertical="true"
            android:background="@mipmap/shop_man"
            android:layout_width="@dimen/dp_16"
            android:layout_height="@dimen/dp_15" />

        <TextView
            android:layout_marginLeft="@dimen/dp_44"
            android:layout_centerVertical="true"
            android:text="店员业绩分析"
            android:textColor="@color/c_333333"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" />

        <ImageView
            android:layout_marginRight="@dimen/dp_16"
            android:layout_alignParentRight="true"
            android:layout_centerVertical="true"
            android:background="@mipmap/right_lab"
            android:layout_width="@dimen/dp_6"
            android:layout_height="@dimen/dp_12" />
    </RelativeLayout>
    <RelativeLayout
        android:id="@+id/analyse_sales_rl"
        android:layout_marginTop="@dimen/dp_10"
        android:background="@color/white"
        android:layout_width="match_parent"
        android:layout_height="@dimen/dp_44">

        <ImageView
            android:layout_marginLeft="@dimen/dp_17"
            android:layout_centerVertical="true"
            android:background="@mipmap/sales_analyse"
            android:layout_width="@dimen/dp_16"
            android:layout_height="@dimen/dp_15" />

        <TextView
            android:layout_marginLeft="@dimen/dp_44"
            android:layout_centerVertical="true"
            android:text="商品销量分析"
            android:textColor="@color/c_333333"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" />

        <ImageView
            android:layout_marginRight="@dimen/dp_16"
            android:layout_alignParentRight="true"
            android:layout_centerVertical="true"
            android:background="@mipmap/right_lab"
            android:layout_width="@dimen/dp_6"
            android:layout_height="@dimen/dp_12" />
    </RelativeLayout>

</LinearLayout>