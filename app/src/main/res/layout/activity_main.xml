<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:title="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#f5f5f5"
    tools:context="jh.zkj.com.yf.Activity.MainActivity">


    <View
        android:id="@+id/home_activity_view"
        android:layout_width="match_parent"
        android:layout_height="@dimen/dp_50"
        android:background="#ffffff"
        app:layout_constraintBottom_toBottomOf="parent" />
    
    <View
        android:id="@+id/home_activity_view1"
        app:layout_constraintBottom_toTopOf="@+id/home_activity_view"
        android:background="#00ffffff"
        android:layout_width="match_parent"
        android:layout_height="@dimen/dp_8"/>

    <ImageView
        android:id="@+id/main_activity_home_page"
        android:layout_marginTop="@dimen/dp_7"
        android:layout_marginLeft="@dimen/dp_35"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toTopOf="@+id/home_activity_view"
        android:background="#000000"
        android:layout_width="@dimen/dp_22"
        android:layout_height="@dimen/dp_22" />
    <ImageView
        android:id="@+id/main_activity_price_list"
        android:layout_marginTop="@dimen/dp_7"
        app:layout_constraintRight_toLeftOf="@+id/main_activity_open_bill"
        app:layout_constraintLeft_toRightOf="@+id/main_activity_home_page"
        app:layout_constraintTop_toTopOf="@+id/home_activity_view"
        android:background="#000000"
        android:layout_width="@dimen/dp_22"
        android:layout_height="@dimen/dp_22" />
    <ImageView
        android:id="@+id/main_activity_open_bill"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintRight_toLeftOf="@+id/main_activity_retail"
        app:layout_constraintLeft_toRightOf="@+id/main_activity_price_list"
        app:layout_constraintTop_toTopOf="@+id/home_activity_view1"
        android:background="#000000"
        android:layout_width="@dimen/dp_60"
        android:layout_height="0dp" />
    <ImageView
        android:id="@+id/main_activity_retail"
        android:layout_marginTop="@dimen/dp_7"
        app:layout_constraintRight_toLeftOf="@+id/main_activity_my"
        app:layout_constraintLeft_toRightOf="@+id/main_activity_open_bill"
        app:layout_constraintTop_toTopOf="@+id/home_activity_view"
        android:background="#000000"
        android:layout_width="@dimen/dp_22"
        android:layout_height="@dimen/dp_22" />

    <ImageView
        android:id="@+id/main_activity_my"
        android:layout_marginTop="@dimen/dp_7"
        android:layout_width="@dimen/dp_22"
        android:layout_height="@dimen/dp_22"
        android:layout_marginRight="@dimen/dp_35"
        android:background="#000000"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="@+id/home_activity_view" />
    <TextView
        android:layout_marginTop="@dimen/dp_6"
        android:id="@+id/main_activity_home_text"
        android:textSize="@dimen/sp_10"
        app:layout_constraintRight_toRightOf="@+id/main_activity_home_page"
        app:layout_constraintLeft_toLeftOf="@+id/main_activity_home_page"
        app:layout_constraintTop_toBottomOf="@+id/main_activity_home_page"
        android:text="首页"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content" />
    <TextView
        android:layout_marginTop="@dimen/dp_6"
        android:id="@+id/main_activity_price_list_text"
        android:textSize="@dimen/sp_10"
        android:text="报价单"
        app:layout_constraintRight_toRightOf="@+id/main_activity_price_list"
        app:layout_constraintLeft_toLeftOf="@+id/main_activity_price_list"
        app:layout_constraintTop_toBottomOf="@+id/main_activity_price_list"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content" />
    <TextView
        android:layout_marginTop="@dimen/dp_6"
        android:id="@+id/main_activity_retail_text"
        android:textSize="@dimen/sp_10"
        android:text="零售单"
        app:layout_constraintTop_toBottomOf="@+id/main_activity_retail"
        app:layout_constraintRight_toRightOf="@+id/main_activity_retail"
        app:layout_constraintLeft_toLeftOf="@+id/main_activity_retail"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content" />

    <TextView
        android:id="@+id/main_activity_my_text"
        android:text="我的"
        android:layout_marginTop="@dimen/dp_6"
        android:textSize="@dimen/sp_10"
        app:layout_constraintTop_toBottomOf="@id/main_activity_my"
        app:layout_constraintRight_toRightOf="@id/main_activity_my"
        app:layout_constraintLeft_toLeftOf="@id/main_activity_my"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        />
    <TextView
        android:layout_marginTop="@dimen/dp_6"
        android:id="@+id/main_activity_open_bill_text"
        android:layout_marginBottom="@dimen/dp_7"
        android:textColor="#ffffff"
        android:text="开单"
        android:textSize="@dimen/sp_10"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintLeft_toLeftOf="@+id/main_activity_open_bill"
        app:layout_constraintRight_toRightOf="@+id/main_activity_open_bill"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        />

    <jh.zkj.com.yf.Mview.MainViewPager
        android:id="@+id/home_activity_view_page"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintBottom_toTopOf="@+id/home_activity_view1"
        android:layout_width="match_parent"
        android:layout_height="0dp">
    </jh.zkj.com.yf.Mview.MainViewPager>

</android.support.constraint.ConstraintLayout>
